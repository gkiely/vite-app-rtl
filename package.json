{
  "name": "vite-app",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "npm run fast-install && npm run dev",
    "dev": "vite",
    "fast-install": "npm i --prefer-offline --progress=false --no-audit --ignore-scripts",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test-all": "vitest run",
    "c": "vitest run --coverage"
  },
  "dependencies": {
    "@types/node": "^17.0.42",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.2",
    "happy-dom": "^5.2.0",
    "typescript": "^4.7.3",
    "vite": "^2.9.12",
    "vite-plugin-checker": "^0.4.6",
    "vitest": "^0.14.2"
  }
}
